{
    "manifest_version": 3,
    "name": "Tab Sorter",
    "description": "窗口根据标题字典排序",
    "version": "1.0",
    "action": {
        "default_icon": "sort-ascending.png"
    },
    "background": {
      "service_worker": "background.js"
    },
    "permissions": ["activeTab", "scripting", "tabs"]
  }