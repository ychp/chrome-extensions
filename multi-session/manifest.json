{
    "manifest_version": 3,
    "name": "Multi Session",
    "description": "多登录态管理",
    "version": "1.0",
    "action": {
        "default_popup": "index.html",
        "default_icon": "multi-session.png"
    },
    "permissions": [
        "activeTab",
        "scripting",
        "tabGroups",
        "tabs",
        "storage"
    ]
}