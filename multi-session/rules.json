[
    {
      "id": 1,
      "priority": 1,
      "action": {
        "type": "modifyHeaders",
        "responseHeaders": [
          {
            "header": "set-cookie",
            "operation": "appendPrefix",
            "value": "${groupId}"
          }
        ]
      },
      "condition": {
        "regexFilter": "^.*$",
        "resourceTypes": [
          "main_frame",
          "sub_frame",
          "stylesheet",
          "script",
          "image",
          "object",
          "xml_http_request",
          "other"
        ]
      }
    }
  ]